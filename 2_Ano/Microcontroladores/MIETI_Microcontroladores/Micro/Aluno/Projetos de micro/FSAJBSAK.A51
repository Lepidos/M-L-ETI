	NAME NEWPASS
	PUBLIC INICIOPASS1
	NEWPASS SEGMENT CODE 
	RSEG NEWPASS
	JMP INICIOPASS1
		
	EXTRN CODE (INICIOREAD)
		
// REDEFENIR NOVA PASS

INICIOPASS1:
	MOV R0,#40H
	MOV R2,#4
	MOV A,R0
	MOV R1,A
INICIO:
	USING 0
	PUSH AR1
	LCALL INICIOREAD
	POP AR1
	MOV A,R0
	MOV @R1,A
	INC R1
	DJNZ R2,INICIO
FIM:
	RET
	END