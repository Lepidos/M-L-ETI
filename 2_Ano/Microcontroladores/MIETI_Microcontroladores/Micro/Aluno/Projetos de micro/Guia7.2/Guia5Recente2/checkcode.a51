	NAME CHECK
	PUBLIC INICIOCHECK
	CHECKCODE SEGMENT CODE 
	RSEG CHECKCODE
	JMP INICIOCHECK
	
// VERIFICAR SE O CODIGO INTRODUZIDO ESTÁ CORRETO
	
INICIOCHECK:
	MOV R2,#4
START:
	MOV B,@R0
	CLR A
	MOV A,@R1
	CJNE A,B,FALSO
	INC R1
	INC R0
	DJNZ R2,START
	MOV R0,#1
	JMP FIM

FALSO:
	MOV R0,#0
FIM:
	 RET 
END	