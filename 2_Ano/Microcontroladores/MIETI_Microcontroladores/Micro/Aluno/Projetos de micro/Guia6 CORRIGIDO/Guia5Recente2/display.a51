	NAME DISPLAY
	PUBLIC INICIODISPLAY
	DISPLAYCODE SEGMENT CODE 
	RSEG DISPLAYCODE
	JMP INICIODISPLAY
	
	TABLECODE SEGMENT CODE
	RSEG TABLECODE
	TABLE: DB 0CH,0CFH,54H,46H,87H,26H,24H,4FH,4H,6H,0FFH,0DH,82H,15H,34H

INICIODISPLAY:
	MOV DPTR,#TABLE
	MOV A,R0
	MOVC A,@A+DPTR
	MOV P1,A

	
	FIM: RET
END					