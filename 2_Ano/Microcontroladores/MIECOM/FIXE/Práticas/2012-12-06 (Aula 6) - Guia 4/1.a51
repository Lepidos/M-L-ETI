//#include<89C51Rx2.Inc>

	EXTRN   CODE (CONFIG_RS232,RECEBER_RS232,ENVIAR_RS232)
	CSEG AT 0H
	SJMP START
	CSEG AT 30H

START:
	CALL CONFIG_RS232	
	MOV P1,#35H
	MOV R2,#11
    MOV DPTR, #STRING
			
LOOP:						
	CLR A
	MOVC A, @A+DPTR	
	CALL ENVIAR_RS232
	INC DPTR		 
	DJNZ R2, LOOP

STRING: 
	DB "MICROS 2012"

END
