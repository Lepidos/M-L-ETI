node n1 {
    type lanswitch
    network-config {
	hostname n1
	!
    }
    canvas c1
    iconcoords {378.0 293.0}
    labelcoords {378.0 317.0}
    interface-peer {e0 n2}
    interface-peer {e1 n7}
    interface-peer {e2 n6}
    interface-peer {e3 n5}
    interface-peer {e5 n4}
    interface-peer {e4 n3}
}

node n2 {
    type router
    model PC
    network-config {
	hostname n2
	!
	interface eth0
	!
    }
    canvas c1
    iconcoords {286.0 183.0}
    labelcoords {286.0 215.0}
    interface-peer {eth0 n1}
    custom-config {
	custom-config-id service:DHCPClient
	custom-command DHCPClient
	config {
	files=('startdhcpclient.sh', )
	starttime=15
	}
    }
    custom-config {
	custom-config-id service:DHCPClient:startdhcpclient.sh
	custom-command startdhcpclient.sh
	config {
	/sbin/dhclient -nw -pf /var/run/dhclient-eth0.pid -lf /var/run/dhclient-eth0.lease eth0
	
	}
    }
    services {DefaultRoute DHCPClient}
}

